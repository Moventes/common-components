{
  "name": "ng-packaged",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "cpr dist/common-components src/mv-common-components && ng build packages && rimraf src/mv-common-components",
    "build:lib": "rimraf dist && ng-packagr -p common-components/package.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "link": "yarn install && npm run build:lib && cd dist/common-components && yarn link",
    "link:npm": "npm install && npm run build:lib && cd dist/common-components && npm link",
    "publish": "yarn install &&  npm run build:lib && echo \"Checking if you are logged in NPM registry (user: moventes, email: dev@moventes.com, pwd: *******)...\" && npm whoami && npm publish dist/common-components",
    "publish:npm": "npm install &&  npm run build:lib && echo \"Checking if you are logged in NPM registry (user: moventes, email: dev@moventes.com, pwd: *******)...\" && npm whoami && npm publish dist/common-components",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "compodoc-serve": "./node_modules/.bin/compodoc -s"
  },
  "dependencies": {
    "@angular/common": "6.0.0-rc.5",
    "@angular/compiler": "6.0.0-rc.5",
    "@angular/core": "6.0.0-rc.5",
    "@angular/forms": "6.0.0-rc.5",
    "@angular/http": "6.0.0-rc.5",
    "@angular/platform-browser": "6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "6.0.0-rc.5",
    "@angular/router": "6.0.0-rc.5",
    "@ngx-translate/core": "9.0.2",
    "circular-json": "^0.5.1",
    "core-js": "^2.4.1",
    "google-libphonenumber": "^3.1.4",
    "moment": "^2.20.1",
    "rxjs": "^6.0.0-beta.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.0",
    "@angular-devkit/schematics": "^0.5.0",
    "@angular/cli": "6.0.0-rc.6",
    "@angular/compiler-cli": "6.0.0-rc.5",
    "@angular/language-service": "6.0.0-rc.5",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~9.6.0",
    "codelyzer": "~4.3.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "3.0.0-rc.2",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.0",
    "tsickle": "^0.27.3",
    "tslib": "^1.7.1",
    "tslint": "~5.9.0",
    "typescript": "~2.7.2"
  }
}
